function y_ddot = assemble_odefun(symbolic_y_ddot, y_old, y_dot_old, u_0)
    
    y_ddot = symbolic_y_ddot;
    y_ddot = subs(y_ddot, alpha, y_old(1));
    y_ddot = subs(y_ddot, beta, y_old(2));
    y_ddot = subs(y_ddot, alpha_dot, y_dot_old(1));
    y_ddot = subs(y_ddot, beta_dot, y_dot_old(2));
    y_ddot = subs(y_ddot, u, u_0);

end